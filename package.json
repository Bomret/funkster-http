{
  "name": "funkster-http",
  "version": "0.0.0",
  "description": "Funkster is a compositional server library. This package contains the basic http types and combinators.",
  "homepage": "https://github.com/Bomret/funkster-http",
  "license": "Unlicense",
  "author": "Stefan Reichel <dev@bomret.com> (http://bomret.com)",
  "repository": {
    "type": "git",
    "url": "git@github.com:Bomret/funkster-http.git"
  },
  "bugs": "https://github.com/Bomret/funkster-http/issues",
  "keywords": [
    "server",
    "compose",
    "composition",
    "functional",
    "funkster",
    "http",
    "web"
  ],
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "clean": "rimraf lib",
    "lint": "tslint --force --format verbose \"src/**/*.ts\"",
    "build": "npm run clean && npm run lint && echo Using TypeScript && tsc --version && tsc --pretty",
    "test": "npm run build && mocha --compilers ts:ts-node/register --recursive test/**/*.tests.ts",
    "watch": "npm run build -- --watch",
    "watch:test": "npm run test -- --watch"
  },
  "dependencies": {
    "es6-promise": "^4.0.5",
    "finalhandler": "^0.5.1",
    "funkster-core": "^0.5.1",
    "parseurl": "^1.3.1",
    "path-to-regexp": "^1.7.0",
    "raw-body": "^2.1.7"
  },
  "devDependencies": {
    "@types/finalhandler": "^0.0.30",
    "@types/mocha": "2.2.29",
    "@types/node": "6.0.31",
    "@types/path-to-regexp": "^1.0.29",
    "mocha": "^3.0.1",
    "rimraf": "^2.5.4",
    "ts-node": "^1.7.0",
    "tslint": "3.15.1",
    "typescript": "rc"
  },
  "engines": {
    "node": ">=0.12"
  }
